<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.*?>

<StackPane fx:id="root" minHeight="594.0" minWidth="350.0" prefHeight="594.0" prefWidth="350.0" xmlns:fx="http://javafx.com/fxml" fx:controller="de.saxsys.projectiler.ProjectilerController">
  <!-- TODO Add Nodes -->
  <children>
    <StackPane id="StackPane" fx:id="timePane">
      <children>
        <ImageView fx:id="cardImage" fitHeight="0.0" fitWidth="0.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="TOP_CENTER">
          <image>
            <Image url="@projectiler_karte.png" />
          </image>
        </ImageView>
        <Label id="fromTimeLable" fx:id="fromTimeLabel" alignment="CENTER" prefWidth="171.0" text="Label" textAlignment="CENTER" translateX="51.0" translateY="-150.0" StackPane.alignment="CENTER">
          <font>
            <Font size="30.0" fx:id="x2" />
          </font>
        </Label>
        <Label alignment="CENTER" font="$x2" prefWidth="93.0" text="Von:" textAlignment="CENTER" translateX="-100.0" translateY="-150.0" />
        <Label id="fromTimeLable" fx:id="toTimeLabel" alignment="CENTER" font="$x2" prefWidth="171.0" text="Label" textAlignment="CENTER" translateX="51.0" translateY="-60.0" />
        <Label alignment="CENTER" font="$x2" prefWidth="93.0" text="Bis:" textAlignment="CENTER" translateX="-100.0" translateY="-60.0" />
      </children>
    </StackPane>
    <ImageView fx:id="timeImage" fitHeight="0.0" fitWidth="0.0" pickOnBounds="true" preserveRatio="true" translateY="280.0" y="0.0" StackPane.alignment="TOP_CENTER">
      <image>
        <Image url="@projectiler_uhr.png" />
      </image>
    </ImageView>
    <ChoiceBox fx:id="projectChooser" maxWidth="295.0" minHeight="50.0" minWidth="295.0" prefWidth="295.0" translateY="0.0">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ChoiceBox>
    <Label text="Label" translateY="150.0">
      <font>
        <Font size="20.0" />
      </font>
    </Label>
    <VBox id="VBox" fx:id="loginVbox" alignment="CENTER" maxHeight="-Infinity" maxWidth="300.0" spacing="5.0" style="-fx-background-color:whitesmoke&#10;" translateY="0.0" visible="false">
      <children>
        <Label font="$x2" text="Projectiler" />
        <TextField fx:id="usernameField" minHeight="50.0" prefWidth="200.0" promptText="Benutzername" style="-fx-font-width:15;&#10;" />
        <PasswordField fx:id="passwordField" minHeight="50.0" prefWidth="200.0" promptText="Passwort" />
        <HBox id="HBox" alignment="CENTER" spacing="30.0">
          <children>
            <Button fx:id="loginButton" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onLoginButtonPressed" text="Ok">
              <font>
                <Font size="12.0" fx:id="x1" />
              </font>
            </Button>
            <Button id="loginButton" font="$x1" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onCloseAction" text="Cancel" />
          </children>
        </HBox>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <StackPane.margin>
        <Insets />
      </StackPane.margin>
    </VBox>
    <ImageView fitHeight="0.0" fitWidth="0.0" onMouseClicked="#onCloseButtonClicked" pickOnBounds="true" preserveRatio="true" translateX="140.0" translateY="265.0">
      <image>
        <Image url="@projectiler_close.png" />
      </image>
    </ImageView>
  </children>
</StackPane>
